{
    "name": "patchit",
    "version": "0.0.0",
    "command_name": "patchit",
    "extension_author": "c2biz",
    "original_author": "ScriptIdiot",
    "repo_url": "https://github.com/sliverarmory/BOF-patchit",
    "help": "patch, check and revert AMSI and ETW for x64 process",
    "depends_on": "coff-loader",
    "entrypoint": "go",
    "files": [
        {
            "os": "windows",
            "arch": "amd64",
            "path": "patchit.x64.o"
        }
    ],
    "arguments": [
        {
            "name": "mode",
            "desc": "1 = check, 2 = all, 3 = amsi, 4 = etw, 5 = revertAll, 6 = revertAmsi, 7 = revertEtw",
            "type": "integer",
            "optional": false
        }
    ]
}